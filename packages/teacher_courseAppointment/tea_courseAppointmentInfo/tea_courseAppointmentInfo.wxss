page{
  background-color: #FBFBFC;
}
.navi {
  width: 100%;
  background:linear-gradient(90deg,rgba(20,203,162,1) 0%,rgba(111,238,209,1) 100%);
  position: fixed;
  z-index: 99;
  top: 0;
  left: 0;
  display: flex;
}

.navi_content {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: flex-end;
}

.navi_back {
  height: 100%;
  width: 93rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.navi_back_img {
  width: 27.11rpx;
  height: 27.11rpx;
}

.navi_title {
  margin-left: 0;
  color: white;
  font-size: 34rpx;
  font-weight: bold;
  max-width: 474rpx;
  height: 100%;
  display: flex;
  align-items: center;
}


.tablehHeaderView {
  height: 45rpx;
  margin-top: 39rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.tablehHeader_circle {
  margin-left: 30rpx;
  width: 11rpx;
  height: 11rpx;
  border-radius: 50%;
  border: 4rpx solid #14CBA2;
}

.tablehHeader_title {
  margin-left: 20rpx;
  color: #272727;
  font-size: 32rpx;
  font-weight: bold;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.tablehHeader_count {
  color: #2DC1A0;
  font-size: 32rpx;
  font-weight: bold;
}

.content{
  margin-top: 180rpx;
  padding-bottom: 20rpx; 
  box-sizing: border-box; 
  height: 100%;
  position: fixed;
} 
::-webkit-scrollbar{
width: 0;
height: 0;
color: transparent;
}
.flex{
  display: flex;
  width:750rpx;
  height:122rpx;
  background:rgba(255,255,255,1);
  opacity:1;
  flex-direction: row;
  align-items: center;
}
.heng{
