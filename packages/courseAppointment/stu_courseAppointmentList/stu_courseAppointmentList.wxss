/* pages/courseAppointment/stu_courseAppointmentList/stu_courseAppointmentList.wxss */
page{
  background-color: #FBFBFC;
}
.flex_row_alimenterCenter {
  display: flex;
  flex-direction: row;
  align-items: center;
}
.oneLine {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}
.navi {
  width: 100%;
  background:linear-gradient(90deg,rgba(20,203,162,1) 0%,rgba(111,238,209,1) 100%);
  position: fixed;
  z-index: 99;
  top: 0;
  left: 0;
  display: flex;
}

.navi_content {
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: flex-end;
}

.navi_back {
  height: 100%;
  width: 93rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}

.navi_back_img {
  width: 27.11rpx;
  height: 27.11rpx;
}

.navi_control_selected {
  margin-bottom: 21rpx;
  display: flex;
  align-items: flex-end;
  justify-content: center;
  width: 182rpx;
  height: 53rpx;
  color: white;
  font-size: 38rpx;
  font-weight: bold;
}

.navi_concrol {
  margin-bottom: 21rpx;
  display: flex;
  align-items: flex-end;
  justify-content: center;
  width: 182rpx;
  height: 53rpx;
  color: white;
  font-size: 32rpx;
}

.contentView {
  display: flex;
  flex-direction: column;
}

.sectionHeader {
  height: 103rpx;
  width: 100%;
  margin-top: 10rpx;
  display: flex;
  align-items: center;
  flex-direction: row;
}

.sectionCircle {
  margin-left: 30rpx;
  width: 11rpx;
  height: 11rpx;
  border-radius: 7.5rpx;
  border: 4rpx solid #14CBA2;
}

.sectionTitle {
  margin-left: 20rpx;
  color: #272727;
  font-size: 32rpx;
  font-weight: bold;
}

.appointmentCell {
  background-color: white;
  border-radius: 14rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  height: 155rpx;
  margin-left: 30rpx;
  width: 690rpx;
}

.appointmentTitle {
  color: #272727;
  font-size: 30rpx;
  margin-left: 35rpx;
  max-width: 340rpx;
}

.appointmentButton {
  background-color: #14CBA2;
  height: 56rpx;
  width: 105rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  font-size: 24rpx;
  margin-right: 33rpx;
  border-radius: 28rpx;
}

.courseIntroButton {
  margin-right: 27rpx;
  width: 140rpx;
  height: 54rpx;
  border-radius: 28rpx;
  border: 2rpx solid #F2F2F2;
  display: flex;
  align-items: center;
  justify-content: center;
  color: #BBBBBB;
  font-size: 24rpx;
}
