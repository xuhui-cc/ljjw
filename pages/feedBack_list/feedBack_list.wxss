/* pages/feedBack_list/feedBack_list.wxss */

/* 导航栏 */
page{
  background-color: #fbfbfc;
}
.navigationBar {
  position: fixed;
  width:750rpx;
  height:176rpx;
  background:linear-gradient(90deg,rgba(20,203,162,1) 0%,rgba(111,238,209,1) 100%);
  opacity:1;
  display: flex;
  z-index: 100;
}
.navigationBar_contentView {
  width: 100%;
  display: flex;
  flex-direction: row;
}

.navibar_backImageView {
  height: 100%;
  width: 100rpx;
  margin-left: 10rpx;
  display: flex;
  align-items: center;
}
.navibar_backImage {
  width: 27rpx;
  height: 27rpx;
  margin-left: 22rpx;
}

.topMenuView {
  height: 100%;
  /* width: 178rpx; */
  display: flex;
  justify-content: center;
  align-items: flex-end;
  /* padding-bottom: 21rpx; */
  font-size: 32rpx;
  color: white;
}

.topMenuViewVisited {
  height: 100%;
  /* width: 178rpx; */
  display: flex;
  justify-content: center;
  align-items: flex-end;
  color: white;
  font-size: 38rpx;
  font-weight: bold;
}
.toMenuTitle {
  margin-bottom: 21rpx;
}

.topMenuDot {
  background-color: #f14444;
  font-size: 22rpx;
  color: white;
  border-radius: 15rpx;
  width: 42rpx;
  height: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 3rpx;
  margin-bottom: 52rpx;
}


.contentView {
  display: flex;
  flex-direction: column;
}

/* item */
.itemView {
  width: 100%;
  height: 112rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  background-color: white;
}

.itemView_subView {
  display: flex;
  flex-direction: row;
  height: 100%;
  align-items: center;
}

.green {
  width: 11rpx;
  height: 11rpx;
  border-radius: 50%;
  margin-left: 30rpx;
  border: 4rpx solid #74BB71;
}

.item_title {
  font-size: 33rpx;
  font-weight: bold;
  color: #272727;
  margin-left: 17rpx;
}

.item_nextImage {
  margin-right: 30rpx;
  width: 33rpx;
  height: 33rpx;
}

.secondType_collectionView {
  background-color: white;
  width: 100%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  padding-bottom: 53rpx;
}

.secondTypeView {
  width: 214rpx;
  height: 90rpx;
  border-radius: 7rpx;
  background-color: #f8f8f8;
  display: flex;
  align-items: center;
  justify-content: center;
}

.secondType_title {
  color: #828282;
  font-size: 26rpx;
  text-align: center;
  max-width: 194rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

/* 反馈处理 */
.feedBackCell {
  background-color: white;
  display: flex;
  flex-direction: column;
  position: relative;
}

.feedBack_BackInfo {
  width: 631rpx;
  height: 106rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.feedBack_readStatus {
  width: 65rpx;
  height: 34rpx;
  font-size: 22rpx;
  font-weight: bold;
  color: white;
  justify-content: center;
  display: flex;
  align-items: center;
  border-radius: 0 8rpx 8rpx 0;
}

.feedBack_createTime {
  margin-left: 23rpx;
  font-size: 34rpx;
  font-weight: bold;
  color: #272727;
  max-width: 300rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.feedBack_type {
  margin-left: 21rpx;
  font-size: 26rpx;
  font-weight: bold;
  color: #272727;
  max-width: 222rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.feedBack_dealType {
  position: absolute;
  top: 0;
  right: 0;
  width: 119rpx;
  height: 119rpx;
}

.feedBack_content {
  margin-top: 6rpx;
  margin-left: 30rpx;
  width: 690rpx;
  font-size: 26rpx;
  color: #dbdbdb;
}

.feedBack_openButton {
  height: 100rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.feedBack_bottomLine {
  height: 36rpx;
  margin-left: 30rpx;
  width: 690rpx;
  border-bottom: 1rpx dotted #e9e9e9;
}
.feedBack_bottom {
  width: 100%;
  height: 64rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.feedBack_bottomArrow {
  width: 23rpx;
  height: 23rpx;
}

.feedBack_bottomText {
  margin-left: 15rpx;
  font-size: 24rpx;
  color: #b4b4b4;
}

.feedBack_stuPicture_collectionView {
  margin-left: 30rpx;
  width: 643rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.feedBack_stuPicture {
  width: 200rpx;
  height: 200rpx;
  background-color: black;
}

.feddBack_stuPicture_bottomLine {
  height: 50rpx;
  margin-left: 30rpx;
  width: 690rpx;
  border-bottom: 1rpx solid #ececec;
}

.feddBack_dealTimeView {
  height: 113rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.feedBack_dealTime_image {
  width: 31rpx;
  height: 31rpx;
  margin-left: 30rpx;
}

.feedBack_dealTime_time {
  margin-left: 27rpx;
  color: #272727;
  font-size: 34rpx;
  max-width: 632rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.feedBack_dealContent {
  margin-left: 30rpx;
  width: 664rpx;
  display: flex;
  flex-wrap: wrap;
  font-size: 26rpx;
}

.feedBack_dealPicture_bottomLine {
  height: 56rpx;
  width: 100%;
  border-bottom: 0.5rpx solid #ececec;
}

.evaluated_scoredView {
  margin-top: 12rpx;
  height: 116rpx;
  width: 100%;
  display: flex;
  align-items: center;
  flex-direction: row;
}

.evaluate_scoreTitle {
  font-size: 28rpx;
  color: #272727;
  margin-left: 30rpx;
}

.evaluated_content {
  color: #8d8d8d;
  font-size: 28rpx;
  margin-left: 30rpx;
  width: 690rpx;
  display: flex;
  flex-wrap: wrap;
}

.feedBack_evaluateTextArea_backgroundView {
  margin-left: 30rpx;
  width: 690rpx;
  border-radius: 14rpx;
  background-color: #f6f6f6;
  height: 179rpx;
}

.feedBack_evaluateTextarea {
  margin-left: 27rpx;
  margin-top: 24rpx;
  width: 636rpx;
  height: 131rpx;
  font-size: 28rpx;
  color: black;
}

.feedBack_evaluateTextarea_placehoulder {
  font-size: 28rpx;
  color: #b4b4b4;
}

.feedBack_evaluate_submitButton {
  margin-left: 30rpx;
  width: 690rpx;
  height: 90rpx;
  border-radius: 45rpx;
  background-color: #dedede;
  font-size: 32rpx;
  color: white;
  margin-top: 72rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}