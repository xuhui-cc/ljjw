/* pages/tea_feedBack_list/tea_feedBack_list.wxss */
page{
  background-color: #fbfbfc;
}

.flex_row {
  display: flex;
  flex-direction: row;
  width: 100%;
}

.nullImage {
  position: fixed;
  width: 100%;
  height: 300rpx;
  top:50%;
  margin-bottom: 150rpx;
}

.navigationBar {
  position: fixed;
  width:750rpx;
  height:176rpx;
  background:linear-gradient(90deg,rgba(20,203,162,1) 0%,rgba(111,238,209,1) 100%);
  opacity:1;
  display: flex;
  z-index: 100;
}
.navigationBar_contentView {
  width: 100%;
  display: flex;
  flex-direction: row;
}

.navibar_backImageView {
  height: 100%;
  width: 100rpx;
  margin-left: 10rpx;
  display: flex;
  align-items: center;
}
.navibar_backImage {
  width: 27rpx;
  height: 27rpx;
  margin-left: 22rpx;
}

.topMenuView {
  height: 100%;
  /* width: 178rpx; */
  display: flex;
  justify-content: center;
  align-items: flex-end;
  /* padding-bottom: 21rpx; */
  font-size: 32rpx;
  color: white;
}

.topMenuViewVisited {
  height: 100%;
  /* width: 178rpx; */
  display: flex;
  justify-content: center;
  align-items: flex-end;
  color: white;
  font-size: 38rpx;
  font-weight: bold;
}
.toMenuTitle {
  margin-bottom: 21rpx;
}

.topMenuDot {
  background-color: #f14444;
  font-size: 22rpx;
  color: white;
  border-radius: 15rpx;
  width: 42rpx;
  height: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  margin-left: 3rpx;
  margin-bottom: 52rpx;
}

.selectTypeView {
  height: 94rpx;
  width: 100%;
  position: fixed;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: #FBFBFC;
  z-index: 99;
}

.selectTypeImage {
  width: 34rpx;
  height: 33rpx;
  margin-left: 30rpx;
}

.selectTypeTitle {
  margin-left: 14rpx;
  font-size: 32rpx;
  font-weight: bold;
  color: #2DC1A0;
}

.contentView {
  display: flex;
  flex-direction: column;
}

.feedBackCell {
  background-color: white;
  display: flex;
  flex-direction: column;
  position: relative;
}

.feedBack_BackInfo {
  width: 629rpx;
  height: 58rpx;
  margin-top: 29rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
}

.feedBack_readStatus {
  width: 65rpx;
  height: 34rpx;
  font-size: 22rpx;
  font-weight: bold;
  color: white;
  justify-content: center;
  display: flex;
  align-items: center;
  border-radius: 8rpx;
  margin-left: 30rpx;
  background-color: #F14444;
}

.feedBack_createTime {
  margin-left: 30rpx;
  font-size: 28rpx;
  font-weight: bold;
  color: #C1C1C1;
  max-width: 300rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.feedBack_type {
  /* margin-left: 21rpx; */
  font-size: 32rpx;
  font-weight: bold;
  color: #272727;
  max-width: 518rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.feedBack_dealType {
  position: absolute;
  top: 0;
  right: 0;
  width: 121rpx;
  height: 120rpx;
}

.feedBack_content {
  margin-top: 30rpx;
  margin-left: 30rpx;
  width: 690rpx;
  font-size: 28rpx;
  color: #8D8D8D;
  line-height: 40rpx;
}

.feedBack_openButton {
  height: 100rpx;
  width: 100%;
  display: flex;
  flex-direction: column;
}

.feedBack_bottomLine {
  height: 36rpx;
  margin-left: 30rpx;
  width: 690rpx;
  border-bottom: 1rpx dotted #e9e9e9;
}
.feedBack_bottom {
  width: 100%;
  height: 64rpx;
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: center;
}
.feedBack_bottomArrow {
  width: 23rpx;
  height: 23rpx;
}

.feedBack_bottomText {
  margin-left: 15rpx;
  font-size: 24rpx;
  color: #b4b4b4;
}

.feedBack_stuPicture_collectionView {
  margin-left: 93rpx;
  width: 629rpx;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
}

.feedBack_stuPicture {
  width: 200rpx;
  height: 200rpx;
}

.feddBack_stuPicture_bottomLine {
  height: 50rpx;
  margin-left: 30rpx;
  width: 690rpx;
  border-bottom: 1rpx solid #ececec;
}

.feedBackProcessCell {
  background-color: white;
  display: flex;
  position: relative;
  flex-direction: column;
  width: 100%;
}

.feedBackProcess_dotLine {
  width: 51rpx;
  height: 100%;
  border-right: 3rpx dashed #C5C5C5;
  position: absolute;
}

.feedBackProcess_infoView {
  display: flex;
  width: 100%;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
}
.flex_itemAlignCenter {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.feedBackProcess_role {
  width: 43rpx;
  height: 42rpx;
  border-radius: 13rpx;
  margin-left: 30rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  color: white;
  font-size: 26rpx;
  font-weight: bold;
  z-index: 2;
}

.feedBackProcess_username {
  margin-left: 20rpx;
  max-width: 120rpx;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.feedBack_dealTime_time {
  margin-left: 38rpx;
  color: #8D8D8D;
  font-size: 28rpx;
  max-width: 350rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.feedBackProcess_status {
  margin-right: 30rpx;
  font-size: 28rpx;
  font-weight: bold;
}

.feedBack_dealContent {
  margin-left: 93rpx;
  width: 627rpx;
  display: flex;
  flex-wrap: wrap;
  font-size: 30rpx;
  line-height: 45rpx;
}

.feedBack_dealPicture_bottomLine {
  height: 56rpx;
  width: 100%;
  border-bottom: 0.5rpx solid #ececec;
}

.feedBackDealSure {
  color: white;
  font-size: 32rpx;
  background-color: #2DC1A0;
  margin-top: 66rpx;
  height: 80rpx;
  width: 311rpx;
  margin-left: 45rpx;
  border-radius: 40rpx;
}